{
  "name": "exness-test",
  "title": "Exness Test",
  "description": "",
  "author": "Dmitry Popenov",
  "version": "1.0.0",
  "scripts": {
    "copyenv:dev": ".\"/scripts/copyenv.sh\" dev",
    "copyenv:production": ".\"/scripts/copyenv.sh\" production",
    "delay": "node ./node_modules/npm-delay 10000",
    "dev": "npm-run-all copyenv:dev webpack:prepare --parallel dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.js --watch --progress --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.js --watch ./static",
    "dev:client:build": "node ./webpack/webpack-dev-server.js",
    "postinstall": "chmod 764 ./scripts/copyenv.sh && chmod 764 ./scripts/tools.sh",
    "production": "npm-run-all copyenv:production webpack:prepare production:tsc production:client:build production:server:build production:server:start",
    "production:build": "npm-run-all production:client:build pdoroduction:server:build",
    "production:start": "npm run production:server:start",
    "production:client:build": "webpack -p --config ./webpack/webpack.config.client.production.js --colors --display-error-details",
    "production:server:build": "webpack --config ./webpack/webpack.config.server.production.js --colors --display-error-details",
    "production:server:start": "node ./src/server/start.js",
    "production:tsc": "tsc --sourceMap false -p .",
    "start": "npm run production",
    "ts": "npm-run-all tsc tslint",
    "tsc": "tsc -p .",
    "tslint": "tslint -c tslint.json 'src/**/*.ts*'",
    "tsw": "tsc -w",
    "webpack:prepare": "tsc -p ./webpack"
  },
  "resolutions": {
    "@types/react": "16.4.18"
  },
  "dependencies": {
    "autobind-decorator": "2.1.0",
    "clone": "2.1.2",
